  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=development",
    "test": "node index"
  },
  "keywords": [
    "react-jsx-template",
    "scoped styles",
    "CSS",
    "JSX",
    "styled components",
    "shadow DOM",
    "encapsulation",
    "template",
    "shadow",
    "root"
  ],
  "author": "Peter Collins",
  "license": "ISC",
  "devDependencies": {
    "@material-ui/core": "^4.10.1",
    "babel-loader": "^8.1.0",
    "jss": "^10.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.10.1",
    "jss": "^10.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "files": [
    "index.js",
    "./src/$ShadowRoot.js"
  ]
}
